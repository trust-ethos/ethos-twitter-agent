{
  "lock": false,
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check **/*.ts && deno check **/*.tsx",
    "cli": "echo \"import '\\$fresh/src/dev/cli.ts'\" | deno run --unstable-kv --allow-net --allow-read --allow-write --allow-env --allow-run --allow-sys -",
    "manifest": "deno task cli manifest $(pwd)",
    "start": "deno run -A --unstable-kv main.ts",
    "dev": "deno run -A --unstable-kv --watch main.ts",
    "test": "deno test -A --unstable-kv",
    "dashboard": "cd dashboard && deno task start",
    "dashboard-build": "cd dashboard && deno task build",
    "dashboard-preview": "cd dashboard && deno task preview",
    "build": "deno run -A --unstable-kv dev.ts build",
    "preview": "deno run -A --unstable-kv main.ts",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@std/": "https://deno.land/std@0.208.0/",
    "oak": "https://deno.land/x/oak@v12.6.1/mod.ts",
    "dotenv": "https://deno.land/std@0.208.0/dotenv/mod.ts"
  },
  "compilerOptions": {
    "lib": ["deno.ns", "deno.unstable"]
  }
}